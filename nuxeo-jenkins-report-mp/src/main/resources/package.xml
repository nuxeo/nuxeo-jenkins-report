<!-- See documentation at http://doc.nuxeo.com/x/HgIz -->
<package type="addon" name="nuxeo-jenkins-report-mp" version="@VERSION@">
  <title>Nuxeo Jenkins Report</title>
  <description>
    <p>The Nuxeo Jenkins Report addon enables users of the Nuxeo Platform to generate and send reports on the status of the Continuous Integration on Jenkins, directly from the Nuxeo Platform. The plugin has been made for Nuxeo organisation purpose and is shared gracefuly in case you would be interested by the same use case.</p>
  </description>
  <home-page>http://doc.nuxeo.com/x/M43Z</home-page>
  <vendor>Nuxeo</vendor>
  <installer restart="true" />
  <uninstaller restart="true" />
  <!-- <hotreload-support>false</hotreload-support> -->
  <!-- <require-terms-and-conditions-acceptance>false</require-terms-and-conditions-acceptance> -->
  <nuxeo-validation>nuxeo_certified</nuxeo-validation>
  <production-state>production_ready</production-state>
  <supported>false</supported>
  <platforms>
    <platform>server-@DISTRIB_VERSION@</platform>
  </platforms>
  <dependencies>
    <package>nuxeo-jsf-ui</package>
  </dependencies>
  <license>LGPL 2.1</license>
  <license-url>http://www.gnu.org/licenses/lgpl-2.1.html</license-url>
  <visibility>PUBLIC</visibility>
</package>
